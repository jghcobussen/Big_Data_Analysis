      "source": [
        "#Use the energy of the B meson and momentum of the B meson as inputs and\n",
        "#calculate the mass of the B meson (Found by analysis of the daughter products)\n",
        "\n",
        "Eb = Energy(\"H1\", mkaon)+\"+\"+Energy(\"H2\", mkaon)+\"+\"+Energy(\"H3\", mkaon) \n",
        "\n",
        "Pb = TotalMomentum()\n",
        "\n",
        "Mb = \"sqrt(\" + \"(\" + Eb + \")**2 - \" + \"(\" + Pb + \")**2)\"\n",
        "\n",
        "#Now plot a histogram of the range of masses of the B meson.\n",
        "#Does this match what we would expect from theory?\n",
        "\n",
        "c7 = ROOT.TCanvas(\"myCanvasName\",\"The Canvas Title\",800,600)\n",
        "tree.Draw(Mb+\">>histogram7(10000,5000,6500)\")\n",
        "histogram7 = ROOT.gDirectory.Get(\"histogram7\")\n",
        "histogram7.SetTitle(\"Mass of B Meson\")\n",
        "c7.Draw()\n",
        "\n"
      ],
