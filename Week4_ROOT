      "source": [
        "#Function to calculate the energy of a daughter particle \n",
        "\n",
        "c = 2.997*(10**8)\n",
        "mkaon = 493.677\n",
        "\n",
        "def Energy(H, mass):\n",
        "  return \"sqrt(\"+p_tot_daughter(H)+\"**2+(\"+ str(mass) + \")**2)\"\n",
        "\n",
        "Energy(\"H1\", mkaon)\n",
        "\n",
        "c4 = ROOT.TCanvas(\"myCanvasName\",\"The Canvas Title\",800,600)\n",
        "tree.Draw(Energy(\"H1\", mkaon)+\">>histogram4(100,0,40000)\")\n",
        "histogram4 = ROOT.gDirectory.Get(\"histogram4\")\n",
        "histogram4.SetTitle(\"Total Energy of H1\")\n",
        "c4.Draw()"
