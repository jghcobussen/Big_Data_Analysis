      "source": [
        "#use the histogram plotting tools, call your function to find the total momentum and then plot it!\n",
        "momentum = ROOT.RooRealVar(p_tot_daughter(\"H1\"), \"Another Test\", 0, 10000, \"MeV/C\")\n",
        "\n",
        "# We then create a dataset for us to play with\n",
        "data = ROOT.RooDataSet(\"data\",\"data set\", tree, ROOT.RooArgSet(momentum), \"1==1\")\n",
        "\n",
        "# Now we create a canvas, plot our data onto the canvas and draw it:\n",
        "c = ROOT.TCanvas(\"c\",\"c\")\n",
        "frame = momentum.frame()\n",
        "data.plotOn(frame)\n",
        "frame.Draw()\n",
        "c.Draw()\n"
      ],
